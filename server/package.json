{
  "name": "Portfolio",
  "version": "1.1.0",
  "scripts": {
    "test:start": "pm2 start ./Build/core/Portfolio.core.js --name Portfolio --max-memory-restart 200M -f",
    "test": "yarn run lint && yarn run test:start && yarn run stop",
    "lint": "eslint ./src/**/*.{js,ts} --fix",
    "build": "yarn run lint && tsc && cp package.json ./Build/ && cp yarn.lock ./Build/ && cd ./Build && yarn install",
    "dev": "yarn run lint && nodemon src/core/Portfolio.core.ts",
    "start": "pm2 start ./core/Portfolio.core.js --name Portfolio --max-memory-restart 200M -f",
    "stop": "pm2 stop Portfolio"
  },
  "description": "This is the Portfolio Website for Ankan Saha",
  "main": "server.js",
  "repository": "https://github.com/AnkanSaha/Portfolio.git",
  "author": "Ankan Saha",
  "license": "MIT",
  "private": false,
  "keywords": [
    "Portfolio",
    "backend",
    "payment",
    "gateway",
    "api",
    "node"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "yarn": "^1.22.21"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "mongodb": "^6.5.0",
    "outers": "^8.4.5"
  }
}
