{
  "name": "theankan.live",
  "version": "2.3.9",
  "description": "The Official Application of Ankan Saha",
  "main": "portfolio.js",
  "scripts": {
    "updatepack": "npm i express@latest mongoose@latest pug@latest cors@latest pm2@latest nodemon@latest tailwindcss@latest autoprefixer@latest postcss@latest tw-elements@latest bootstrap@latest",
    "dev": "nodemon portfolio.js",
    "start": "pm2 start portfolio.js --name Portfolio --watch",
    "pm2g": "npm install -g pm2",
    "stop": "pm2 stop portfolio.js --name Portfolio",
    "build": "tailwindcss build -i ./static/css/base.css -o ./static/css/tailwind.css -c tailwind.config.js --minifiy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnkanSaha/Portfolio-NodeJS-Version"
  },
  "keywords": [
    "Ankan",
    "Saha"
  ],
  "author": "Ankan Saha",
  "license": "MIT License",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "mongoose": "^7.5.0",
    "pm2": "^5.3.0",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "bootstrap": "^5.3.1",
    "flowbite": "^1.7.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "tw-elements": "^1.0.0-beta2"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "maintainers": [
    "Ankan Saha"
  ],
  "markdown": "github"
}
